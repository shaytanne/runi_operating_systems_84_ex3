{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "debug testfilesystem",
            "type": "shell",
            "command": "gcc",
            "args": [
                "-g",
                "fs.c",
                "testfilesystem.c",
                "-o",
                "debug_test_fs"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "problemMatcher": [
                "$gcc"
            ]
        },
        {
            "label": "debug custom_tests",
            "type": "shell",
            "command": "gcc",
            "args": [
                "-g",
                "fs.c",
                "custom_tests.c",
                "-o",
                "debug_custom_tests"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "problemMatcher": [
                "$gcc"
            ]
        }
        
    ]
}


